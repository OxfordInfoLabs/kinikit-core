{
  "name": "oxil/kinikit-core",
  "version": "0.7",
  "description": "Kinikit - PHP Application development framework",
  "keywords": [
    "PHP",
    "application",
    "mvc",
    "json webservices",
    "webservices",
    "orm",
    "persistence framework"
  ],
  "homepage": "https://kinikit.com",
  "type": "library",
  "require": {
    "php": "^8.1",
    "ext-iconv": "*",
    "ext-libxml": "*",
    "ext-mbstring": "*",
    "mustache/mustache": "*",
    "twig/twig": "*",
    "phpmailer/phpmailer": "^6.0",
    "phpseclib/phpseclib": "*",
    "amphp/parallel": "^2.2",
    "amphp/socket": "*",
    "amphp/http": "*",
    "amphp/http-client": "*",
    "google/cloud-logging": "*",
    "google/appengine-php-sdk": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "autoload": {
    "psr-4": {
      "Kinikit\\Core\\": "src/"
    }
  }
}
